<p id="notice"><%= notice %></p>


<h1>Parks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Neighborhood</th>
      <th>Park Website</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parks.each do |park| %>
      <tr>
        <td><%= link_to park.name, park %></td>
        <td><%= park.neighborhood %></td>
        <td><%= park.park_url %></td>
        <td><%= link_to 'Show', park %></td>
        <td><%= link_to 'Edit', edit_park_path(park) %></td>
        <td><%= link_to 'Destroy', park, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Park', new_park_path %>
